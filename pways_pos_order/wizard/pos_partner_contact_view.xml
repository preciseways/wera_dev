<odoo>
    <data>
        <record id="pos_order_contact_wizard_form" model="ir.ui.view">
            <field name="name">pos.order.connect.wizard.form</field>
            <field name="model">pos.order.connect.wizard</field>
            <field name="arch" type="xml">
                <form >
                    <group>
                        <group>
                            <field name="marks"/>
                        </group>
                        <group>
                        </group>
                    </group>
                    <footer>
                        <button name="action_confirm" string="Confirm" type="object" class="oe_highlight" />
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        <record id="pos_order_contact_wizard_action" model="ir.actions.act_window">
            <field name="name">POS Order Contact</field>
            <field name="res_model">pos.order.connect.wizard</field>
            <field name="view_mode">form</field>
            <field name="type">ir.actions.act_window</field>
            <field name="target">new</field>
        </record>
    </data>
</odoo>
